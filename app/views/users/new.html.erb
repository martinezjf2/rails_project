<h1>Create a New User</h1>

<%= form_for @user do |f| %>
<%=f.label "First Name" %>
  <%=f.text_field :first_name %>
  <%=f.label "Last Name" %>
  <%=f.text_field :last_name %>
<%=f.label "Username" %>
  <%=f.text_field :username %>
  <%=f.label "Password" %>
  <%=f.password_field :password %>

  <%=f.label "Confirm Password"%>
  <%=f.password_field :password_confirmation %>
  <%=f.submit "Sign Up"%>
<%end%>